# platform :ios, '15.0'

target 'doFavor' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for doFavor
  pod 'Alamofire'
  pod 'Kingfisher', '~> 7.0'
  pod 'SwiftyRSA'
  pod 'JWTDecode', '~> 2.6'

  # Add the Firebase pod for Google Analytics
  pod 'Firebase/Analytics'

  pod 'Firebase/Auth'
  pod 'Firebase/Database'

end
